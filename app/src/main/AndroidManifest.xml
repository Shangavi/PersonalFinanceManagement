<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.category">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".expense" />
        <activity android:name=".income" />
        <activity android:name=".addExpense" />
        <activity android:name=".addIncome" />
        <activity android:name=".SubCategory_expense" />
        <activity android:name=".subCategory_income" />
        <activity
            android:name="com.example.user.com.example.user.category.category.UpdateCategoryExpense"
            android:label="@string/title_activity_update_category_expense"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.user.com.example.user.category.category.addSubCategory"
            android:label="@string/title_activity_add_sub_category"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
